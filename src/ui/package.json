{
  "name": "ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run postinstall && npm run build && vite dev",
    "build": "vite build",
    "preview": "npm run build && vite preview",
    "setup-prerendered-pages": "cp -r .svelte-kit/output/prerendered/pages/** build/ && gzip -kf build/*.* build/**/*.* && brotli -kf build/*.* build/**/*.*",
    "serve-prod": "npm run build && npm run setup-prerendered-pages && http-server ./build -p 8000 --proxy http://localhost:8000? --cors --no-dotfiles -g -b -c259200",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prettier": "prettier --write .",
    "e2e": "playwright test --headed --project chromium",
    "postinstall": "svelte-kit sync"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.0",
    "@lucide/svelte": "^0.544.0",
    "@playwright/test": "^1.46.0",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/enhanced-img": "^0.8.4",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/table-core": "^8.21.3",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "eslint": "^9.39.0",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "http-server": "^14.1.1",
    "layerchart": "^2.0.0-next.27",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.43.2",
    "svelte-check": "^4.3.3",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.16",
    "tslib": "^2.8.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vaul-svelte": "^1.0.0-next.7",
    "vite": "^7.1.12"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@internationalized/date": "^3.10.0",
    "bits-ui": "^2.14.2",
    "class-variance-authority": "^0.7.1",
    "embla-carousel-svelte": "^8.6.0",
    "formsnap": "^2.0.1",
    "lucide-react": "^0.552.0",
    "mode-watcher": "^1.1.0",
    "paneforge": "^1.0.2",
    "rxjs": "^7.8.2",
    "svelte-loading-spinners": "^0.3.6",
    "svelte-sonner": "^1.0.5",
    "sveltekit-i18n": "^2.4.2",
    "sveltekit-superforms": "^2.28.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12"
  }
}
