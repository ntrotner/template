{
  "name": "ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "npm run build && vite preview",
    "serve-prod": "npm run build && http-server ./build -p 8000 --proxy http://localhost:8000? --cors --no-dotfiles -g -b",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint ./src",
    "e2e": "playwright test --headed --project chromium",
    "postinstall": "svelte-kit sync"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/enhanced-img": "^0.2.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^8.56.0",
    "@types/node": "^22.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "http-server": "^14.1.1",
    "lucide-svelte": "^0.354.0",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-loading-spinners": "^0.3.6",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "dependencies": {
    "@internationalized/date": "^3.5.2",
    "bits-ui": "^0.19.5",
    "clsx": "^2.1.0",
    "cmdk-sv": "^0.0.15",
    "embla-carousel-svelte": "^8.0.0",
    "formsnap": "^0.5.1",
    "mode-watcher": "^0.2.2",
    "paneforge": "^0.0.2",
    "rxjs": "^7.8.1",
    "svelte-sonner": "^0.3.19",
    "sveltekit-i18n": "^2.4.2",
    "sveltekit-superforms": "^2.8.1",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0",
    "vaul-svelte": "^0.2.4",
    "zod": "^3.22.4"
  }
